{
  "name": "tanzu-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@cds/city": "^1.1.0",
    "@cds/core": "^6.0.0-next.6",
    "@cds/react": "^6.0.0-next.6",
    "@testing-library/jest-dom": "^5.16.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.24",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-lazylog": "^4.5.1",
    "@types/styled-components": "^5.1.23",
    "fuse.js": "^6.6.2",
    "is-ip": "^4.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.31.2",
    "react-lazylog": "^4.5.3",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "react-use-websocket": "^3.0.0",
    "sass": "^1.49.7",
    "styled-components": "^5.3.3",
    "typescript": "^4.7.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "lint": "eslint --ext js,ts,tsx src",
    "format": "prettier --write src",
    "pre-commit": "npm run format && npm run test",
    "generate-api": "openapi --input ../../api/swagger.yaml --output ./src/swagger-api",
    "start": "npm run lint && npm run generate-api && GENERATE_SOURCEMAP=false react-scripts start",
    "build": "npm run lint && npm run generate-api && GENERATE_SOURCEMAP=false react-scripts build",
    "build:prod": "npm ci && npm run build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!@cds)/\" --env=jsdom",
    "eject": "react-scripts eject",
    "build-index": "node src/scripts/contextual-help/build-index.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8008",
  "devDependencies": {
    "@hookform/resolvers": "^2.8.8",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "html-frontmatter": "^1.6.1",
    "msw": "^0.41.1",
    "openapi-typescript-codegen": "^0.20.1",
    "prettier": "^2.6.2",
    "react-step-wizard": "^5.3.11",
    "remove-html-comments": "^1.0.2",
    "yup": "^0.32.11"
  }
}
